{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "dest": "app.js"
        },
        {
            "src": "/products/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/add",
            "dest": "build/index.html"
        },
        {
            "src": "/",
            "dest": "build/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "build/$1"
        }
    ]
}